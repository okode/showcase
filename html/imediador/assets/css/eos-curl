			<script>
			jQuery(document).ready(function($) {
				$('#eos_contact_form').live('submit', function(e){
				
					e.preventDefault();
					var thisform = this;
					var fields = $(this).serialize();
					var url= $(this).attr('action');
					//alert(url);
					$.ajax({
						url: url,
						type: 'POST',
						data: fields,
						success:function(res){
							//salert(res);
							$('.msgs', thisform).html(res);
						}
					});
				});
				
			
			});
			</script>
						
